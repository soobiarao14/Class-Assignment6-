        "print(\"handout\")\n",
        "\n",
        "\n",
        "import random\n",
        "def main():\n",
        "    print(\"\\nğŸ® Welcome to the Ultimate High Low Game! ğŸ®\")\n",
        "    print(\"********************************************\\n\")\n",
        "\n",
        "    try:\n",
        "        total_rounds = int(input(\"How many rounds would you like to play? ğŸ¯ \"))\n",
        "    except ValueError:\n",
        "        print(\"âš ï¸ Invalid input. Defaulting to 5 rounds.\")\n",
        "        total_rounds = 5\n",
        "\n",
        "    your_score = 0\n",
        "\n",
        "    for i in range(total_rounds):\n",
        "        print(f\"\\nğŸ”µ Round {i + 1} of {total_rounds}\")\n",
        "\n",
        "        computer_number: int = random.randint(1, 100)\n",
        "        your_number: int = random.randint(1, 100)\n",
        "\n",
        "        print(f\"ğŸ‘‰ Your number is: {your_number}\")\n",
        "\n",
        "        choice: str = input(\"Do you think your number is 'higher' or 'lower' than the computer's number? \").strip().lower()\n",
        "\n",
        "        higher_and_correct: bool = choice == \"higher\" and your_number > computer_number\n",
        "        lower_and_correct: bool = choice == \"lower\" and your_number < computer_number\n",
        "\n",
        "        if higher_and_correct or lower_and_correct:\n",
        "            print(f\"âœ… Correct! The computer's number was {computer_number}.\")\n",
        "            funny_positive = random.choice([\n",
        "                \"ğŸ”¥ You're on fire!\",\n",
        "                \"ğŸ˜ Pro level guess!\",\n",
        "                \"ğŸ¯ Bullseye!\",\n",
        "                \"ğŸ’ª Strong move!\"\n",
        "            ])\n",
        "            print(funny_positive)\n",
        "            your_score += 1\n",
        "        else:\n",
        "            print(f\"âŒ Oops! The computer's number was {computer_number}.\")\n",
        "            funny_negative = random.choice([\n",
        "                \"ğŸ˜… Better luck next time!\",\n",
        "                \"ğŸ˜‚ That was a close miss!\",\n",
        "                \"ğŸ¤” Maybe trust your instincts more!\",\n",
        "                \"ğŸ˜¬ Not quite right!\"\n",
        "            ])\n",
        "            print(funny_negative)\n",
        "\n",
        "        print(f\"ğŸ† Current score: {your_score}\")\n",
        "        print(\"-\" * 40)\n",
        "\n",
        "    print(\"\\nğŸ‰ Game Over!\")\n",
        "    print(f\"Your final score: {your_score} out of {total_rounds}\")\n",
        "\n",
        "    # Grade system\n",
        "    percentage = (your_score / total_rounds) * 100\n",
        "    if percentage == 100:\n",
        "        grade = \"ğŸ… Legend!\"\n",
        "    elif percentage >= 80:\n",
        "        grade = \"ğŸ¥‡ Excellent!\"\n",
        "    elif percentage >= 60:\n",
        "        grade = \"ğŸ¥ˆ Good Job!\"\n",
        "    elif percentage >= 40:\n",
        "        grade = \"ğŸ¥‰ Not Bad!\"\n",
        "    else:\n",
        "        grade = \"ğŸ˜¢ Needs Practice.\"\n",
        "\n",
        "    print(f\"Your grade: {grade}\")\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()\n"
